<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lnsoft.system.mapper.StandardModelLibraryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="standardModelLibraryResultMap" type="com.lnsoft.system.entity.StandardModelLibrary">
        <id column="id" property="id"/>
        <result column="create_user" property="createUser"/>
        <result column="create_time" property="createTime"/>
        <result column="update_user" property="updateUser"/>
        <result column="update_time" property="updateTime"/>
        <result column="is_deleted" property="isDeleted"/>
        <result column="status" property="status"/>
        <result column="back_num" property="backNum"/>
        <result column="device_category" property="deviceCategory"/>
        <result column="device_category_code" property="deviceCategoryCode"/>
        <result column="maker" property="maker"/>
        <result column="maker_code" property="makerCode"/>
        <result column="brand" property="brand"/>
        <result column="brand_code" property="brandCode"/>
        <result column="series" property="series"/>
        <result column="model" property="model"/>
        <result column="is_accept" property="isAccept"/>
        <result column="dept_name" property="deptName"/>
        <result column="dept" property="dept"/>
        <result column="user_id" property="userId"/>
        <result column="user_name" property="userName"/>
        <result column="phone" property="phone"/>
        <result column="remark" property="remark"/>
        <result column="no_accept_detail" property="noAcceptDetail"/>
        <result column="expected_time" property="expectedTime"/>
        <result column="back_status" property="backStatus"/>
        <result column="is_finish" property="isFinish"/>
    </resultMap>


    <select id="selectStandardModelLibraryPage" resultMap="standardModelLibraryResultMap">
        select * from idevelop_standard_model_library where is_deleted = 0
    </select>

</mapper>
